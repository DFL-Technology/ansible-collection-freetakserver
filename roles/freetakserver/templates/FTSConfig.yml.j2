System:
  FTS_DATABASE_TYPE: SQLite
  FTS_CONNECTION_MESSAGE: {{ connection_message }}
  FTS_OPTIMIZE_API: True
  FTS_MAINLOOP_DELAY: 1
Addresses:
  FTS_COT_PORT: 8087
  FTS_SSLCOT_PORT: 8089
  FTS_DP_ADDRESS: {{ ansible_default_ipv4.address|default(ansible_all_ipv4_addresses[0]) }}
  FTS_USER_ADDRESS: {{ ansible_default_ipv4.address|default(ansible_all_ipv4_addresses[0]) }}
  FTS_API_PORT: 19023
  FTS_FED_PORT: 9000
  FTS_API_ADDRESS: {{ ansible_default_ipv4.address|default(ansible_all_ipv4_addresses[0]) }}
 }}
FileSystem:
  FTS_DB_PATH: /opt/FreeTAKServer.db
  FTS_COT_TO_DB: True
  FTS_MAINPATH: {{ exec_path }}
  FTS_CERTS_PATH: {{ exec_path }}/certs
  FTS_EXCHECK_PATH: {{ exec_path }}/ExCheck
  FTS_EXCHECK_TEMPLATE_PATH: {{ exec_path }}/ExCheck/template
  FTS_EXCHECK_CHECKLIST_PATH: {{ exec_path }}/ExCheck/checklist
  FTS_DATAPACKAGE_PATH: {{ exec_path }}/FreeTAKServerDataPackageFolder
  FTS_LOGFILE_PATH: {{ exec_path }}/Logs
Certs:
  FTS_SERVER_KEYDIR: {{ exec_path }}/certs/server.key
  FTS_SERVER_PEMDIR: {{ exec_path }}/certs/server.pem
  FTS_TESTCLIENT_PEMDIR: {{ exec_path }}/certs/Client.pem
  FTS_TESTCLIENT_KEYDIR: {{ exec_path }}/certs/Client.key
  FTS_UNENCRYPTED_KEYDIR: {{ exec_path }}/certs/server.key.unencrypted
  FTS_SERVER_P12DIR: {{ exec_path }}/certs/server.p12
  FTS_CADIR: {{ exec_path }}/certs/ca.pem
  FTS_CAKEYDIR: {{ exec_path }}/certs/ca.key
  FTS_FEDERATION_CERTDIR: {{ exec_path }}/certs/server.pem
  FTS_FEDERATION_KEYDIR: {{ exec_path }}/certs/server.key
  FTS_CRLDIR: {{ exec_path }}/certs/FTS_CRL.json
  FTS_FEDERATION_KEYPASS: demopassfed
  FTS_CLIENT_CERT_PASSWORD: demopasscert
  FTS_WEBSOCKET_KEY: YourWebsocketKey
