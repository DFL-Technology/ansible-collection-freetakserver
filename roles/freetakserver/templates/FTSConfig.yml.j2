System:
  FTS_DATABASE_TYPE: SQLite
  FTS_CONNECTION_MESSAGE: {{ fts_connection_message }}
  FTS_OPTIMIZE_API: True
  FTS_MAINLOOP_DELAY: 1
Addresses:
  FTS_COT_PORT: 8087
  FTS_SSLCOT_PORT: 8089
  FTS_DP_ADDRESS: {{ fts_ip_address | default(ip_address) }}
  FTS_USER_ADDRESS: {{ fts_ip_address | default(ip_address) }}
  FTS_API_PORT: 19023
  FTS_FED_PORT: 9000
  FTS_API_ADDRESS: {{ fts_ip_address | default(ip_address) }}
FileSystem:
  FTS_DB_PATH: /opt/FreeTAKServer.db
  FTS_COT_TO_DB: True
  FTS_MAINPATH: {{ fts_package_path }}
  FTS_CERTS_PATH: {{ fts_package_path }}/certs
  FTS_EXCHECK_PATH: {{ fts_package_path }}/ExCheck
  FTS_EXCHECK_TEMPLATE_PATH: {{ fts_package_path }}/ExCheck/template
  FTS_EXCHECK_CHECKLIST_PATH: {{ fts_package_path }}/ExCheck/checklist
  FTS_DATAPACKAGE_PATH: {{ fts_package_path }}/FreeTAKServerDataPackageFolder
  FTS_LOGFILE_PATH: {{ fts_package_path }}/Logs
Certs:
  FTS_SERVER_KEYDIR: {{ fts_package_path }}/certs/server.key
  FTS_SERVER_PEMDIR: {{ fts_package_path }}/certs/server.pem
  FTS_TESTCLIENT_PEMDIR: {{ fts_package_path }}/certs/Client.pem
  FTS_TESTCLIENT_KEYDIR: {{ fts_package_path }}/certs/Client.key
  FTS_UNENCRYPTED_KEYDIR: {{ fts_package_path }}/certs/server.key.unencrypted
  FTS_SERVER_P12DIR: {{ fts_package_path }}/certs/server.p12
  FTS_CADIR: {{ fts_package_path }}/certs/ca.pem
  FTS_CAKEYDIR: {{ fts_package_path }}/certs/ca.key
  FTS_FEDERATION_CERTDIR: {{ fts_package_path }}/certs/server.pem
  FTS_FEDERATION_KEYDIR: {{ fts_package_path }}/certs/server.key
  FTS_CRLDIR: {{ fts_package_path }}/certs/FTS_CRL.json
  FTS_FEDERATION_KEYPASS: demopassfed
  FTS_CLIENT_CERT_PASSWORD: demopasscert
  FTS_WEBSOCKET_KEY: YourWebsocketKey
